<%= form_for [@company, @job] do |f| %>
  <%= f.label :title %><br>
  <%= f.text_field :title %><br>
<br>
  <%= f.label :description %><br>
  <%= f.text_field :description %><br>
<br>
  <%= f.label :level_of_interest %><br>
  <%= f.number_field :level_of_interest %><br>
<br>
  <%= f.label :city %><br>
  <%= f.text_field :city %><br>
<br>
  <%= f.submit %>
<% end %>
